<?xml version="1.0"?>
<config>
    <modules>
        <Qaz_All>
            <version>1.0.1</version>
        </Qaz_All>
    </modules>
    <global>
        <helpers>
            <all>
                <class>Qaz_All_Helper</class>
            </all>
        </helpers>
    </global>
    <adminhtml>
        <acl>
            <resources>
                <all>
                    <title>Allow Everything</title>
                </all>
                <admin>
                    <children>
                        <system>
                            <children>
                                <config>
                                    <children>
                                        <all>
                                            <title>qazMagentoExtensions - All</title>
                                        </all>
                                    </children>
                                </config>
                            </children>
                        </system>
                    </children>
                </admin>
            </resources>
        </acl>
    </adminhtml>
</config>
